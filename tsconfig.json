{
  "include": [
    "env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "./**/*.js",
    "./**/*.jsx",
    "tailwind.config.js"
, "server.mjs"  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "types": [
      "node",
      "@shopify/app-bridge-types"
    ]
  }
}
